{
  "name": "cra-template-rb",
  "description": "The official React Boilerplate template for Create React App",
  "version": "1.2.2",
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "engines": {
    "yarn": ">=1.22.10",
    "node": ">=14.0.0"
  },
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "keywords": [
    "react",
    "create-react-app",
    "template",
    "typescript",
    "redux",
    "reduxjs",
    "react-redux",
    "styled-components"
  ],
  "scripts": {
    "// ---DEFAULT CRA---": "",
    "start": "react-scripts start",
    "deploy-staging": "aws s3 sync build/ s3://pickup-staging-admin --profile pickups && aws cloudfront create-invalidation --distribution-id E16RBASFJGXWBH --paths /* --profile pickups",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "// ---TEMPLATE---": "Scripts will be also in the template",
    "start:prod": "yarn run build && serve -s build",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "// ---TESTING TEMPLATE---": "",
    "test:internals": "yarn run test:extract-messages && yarn run test:generators",
    "test:extract-messages": "jest --config=internals/extractMessages/jest.config.js",
    "verify-startingTemplate-changes": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/verify-startingTemplate-changes.ts",
    "test:coverage": "yarn run test --watchAll=false --coverage",
    "test:cra": "yarn run create:cra-app && cd generated-cra-app && yarn run test:generators && yarn run lint && yarn run checkTs && yarn run cleanAndSetup && yarn run lint && yarn run checkTs",
    "// ---BUILDING TEMPLATE---": "",
    "create:npm-package": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-npm-package.script.ts",
    "create:cra-app": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-cra-app.script.ts",
    "// ---PUBLISHING---": "",
    "changelog": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-changelog.script.ts",
    "release": "standard-version",
    "publish:npm": "yarn run create:npm-package && yarn publish .cra-template-rb --non-interactive",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/reportWebVitals.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "@date-io/date-fns": "^2.11.0",
    "@date-io/moment": "^2.11.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/data-grid": "latest",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/x-grid": "4.0.0-alpha.25",
    "@mui/lab": "5.0.0-alpha.61",
    "@mui/material": "^5.2.5",
    "@mui/styles": "^5.2.3",
    "@mui/x-data-grid": "^5.2.1",
    "@reach/router": "^1.3.4",
    "@reduxjs/toolkit": "1.6.0",
    "@types/js-cookie": "^3.0.1",
    "apexcharts": "^3.28.1",
    "axios": "^0.21.4",
    "css-loader": "^6.3.0",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "file-loader": "^6.2.0",
    "fontfaceobserver": "2.1.0",
    "formik": "^2.2.9",
    "fs": "^0.0.1-security",
    "i18next": "20.3.2",
    "i18next-browser-languagedetector": "6.1.2",
    "js-cookie": "^3.0.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react": "17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-app-polyfill": "2.0.0",
    "react-barcodes": "^1.2.0",
    "react-cool-onclickoutside": "^1.7.0",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.3.4",
    "react-helmet-async": "1.0.9",
    "react-i18next": "11.11.1",
    "react-redux": "7.2.4",
    "react-to-pdf": "0.0.14",
    "react-toastify": "^8.0.2",
    "redux-injectors": "1.3.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "1.1.3",
    "reduxsauce": "^1.2.1",
    "sanitize.css": "12.0.1",
    "style-loader": "^3.3.0",
    "styled-components": "5.3.0",
    "swr": "^1.0.1",
    "ts-loader": "^9.2.6",
    "ttf-loader": "^1.0.2",
    "web-vitals": "2.1.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/node-logger": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.7",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/fontfaceobserver": "0.0.6",
    "@types/jest": "26.0.23",
    "@types/node": "14.6.4",
    "@types/react": "17.0.13",
    "@types/react-dom": "17.0.8",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/react-test-renderer": "17.0.1",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.8",
    "@types/styled-components": "5.1.11",
    "@types/testing-library__jest-dom": "5.14.0",
    "chalk": "4.1.1",
    "cross-env": "7.0.3",
    "devmoji": "2.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "3.2.3",
    "git-branch-is": "4.0.0",
    "husky": "7.0.0",
    "i18next-scanner": "3.0.0",
    "inquirer": "7.3.3",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.4",
    "lint-staged": "10.3.0",
    "node-plop": "0.26.2",
    "plop": "2.7.4",
    "prettier": "2.3.2",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "replace-in-file": "6.2.0",
    "rimraf": "3.0.2",
    "serve": "^12.0.0",
    "shelljs": "0.8.4",
    "standard-version": "9.3.0",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "26.5.6",
    "ts-node": "10.0.0",
    "typescript": "^4.4.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
